{
  "name": "@n8n-ai/orchestrator",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "lint": "eslint --ext .ts src",
    "test": "vitest run --exclude src/e2e.test.ts --exclude src/**/*.e2e.test.ts --coverage.enabled=true --coverage.reporter=lcov --coverage.reporter=text",
    "test:e2e": "vitest run src/e2e.test.ts",
    "test:with-server": "concurrently -k -s first \"tsx src/test-server.ts\" \"sleep 2 && vitest run src/e2e.test.ts\"",
    "test:rest:e2e": "concurrently -k -s first \"tsx src/test-server.ts\" \"sleep 2 && vitest run src/rest*.e2e.test.ts\"",
    "test:all": "vitest run --coverage.enabled=true --coverage.reporter=lcov --coverage.reporter=text"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/websocket": "^8.3.1",
    "@n8n-ai/schemas": "workspace:*",
    "fastify": "^4.28.1",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "20",
    "concurrently": "^8.2.2",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "vitest": "^3.2.4"
  }
}
