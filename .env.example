# n8n-ai Environment Variables

# Ports
N8N_PORT=5678
ORCH_PORT=3000
REDIS_PORT=6379
QDRANT_PORT=6333

# n8n Configuration
N8N_ENCRYPTION_KEY=your-encryption-key-here
N8N_HOST=localhost
N8N_WEBHOOK_URL=http://localhost:5678/

# AI Configuration
N8N_AI_ENABLED=true
N8N_AI_ORCHESTRATOR_URL=http://localhost:3000

# Cache Settings
N8N_AI_LOADOPTIONS_TTL_MS=300000

# Logging
LOG_LEVEL=debug

# External Services (add when ready)
# OPENAI_API_KEY=your-openai-api-key
# ANTHROPIC_API_KEY=your-anthropic-api-key