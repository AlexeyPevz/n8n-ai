# n8n Configuration
N8N_PORT=5678
N8N_ENCRYPTION_KEY=your-encryption-key
N8N_HOST=localhost
N8N_WEBHOOK_URL=http://localhost:5678/

# Orchestrator Configuration
ORCH_PORT=3000
REDIS_URL=redis://redis:6379
QDRANT_URL=http://qdrant:6333
N8N_URL=http://n8n:5678

# AI Configuration
# Provider: openai, anthropic, ollama
AI_PROVIDER=openai
AI_API_KEY=your-openai-api-key
AI_MODEL=gpt-4-turbo-preview
AI_TEMPERATURE=0.3
AI_MAX_TOKENS=4000

# Optional: Fallback AI provider
# AI_FALLBACK_PROVIDER=anthropic
# AI_FALLBACK_API_KEY=your-anthropic-api-key
# AI_FALLBACK_MODEL=claude-3-sonnet-20240229

# AI Features
AI_RAG_ENABLED=true
AI_RAG_COLLECTION=n8n-docs
AI_RAG_TOP_K=5
AI_CACHE_ENABLED=true
AI_CACHE_TTL=3600
AI_STREAMING=false

# Security
N8N_AI_API_TOKEN=your-api-token
N8N_AI_RATELIMIT_WINDOW_MS=15000
N8N_AI_RATELIMIT_MAX=60

# Development
LOG_LEVEL=debug
NODE_ENV=development