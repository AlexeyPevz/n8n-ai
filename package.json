{
  "name": "n8n-ai",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "pnpm -r run build",
    "dev": "pnpm -r --stream --parallel run dev",
    "dev:full": "docker compose up -d && pnpm -r --stream --parallel run dev",
    "lint": "pnpm -r run lint",
    "lint:fix": "pnpm -r run lint -- --fix",
    "test": "pnpm -r run test",
    "test:ci": "pnpm run lint && pnpm run test && pnpm run test:golden",
    "test:golden": "cd examples && pnpm test",
    "e2e": "pnpm -C packages/n8n-ai-orchestrator test:e2e",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "check:types": "pnpm -r exec tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "20",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.0.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4"
  }
}
