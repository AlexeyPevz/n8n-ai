{
  "name": "02 Webhook to HTTP",
  "description": "Webhook trigger that calls external API",
  "prompt": "Create webhook endpoint that receives POST data and forwards it to external API",
  "expectedOperations": {
    "version": "v1",
    "ops": [
      {
        "op": "add_node",
        "node": {
          "id": "webhook-1",
          "name": "Webhook",
          "type": "n8n-nodes-base.webhook",
          "typeVersion": 1,
          "position": [250, 300],
          "parameters": {
            "httpMethod": "POST",
            "path": "data-webhook"
          }
        }
      },
      {
        "op": "add_node",
        "node": {
          "id": "http-1",
          "name": "Forward to API",
          "type": "n8n-nodes-base.httpRequest",
          "typeVersion": 4,
          "position": [600, 300],
          "parameters": {
            "method": "POST",
            "url": "https://api.example.com/webhook-data",
            "bodyParametersUi": {
              "parameter": [
                {
                  "name": "data",
                  "value": "={{ $json }}"
                }
              ]
            }
          }
        }
      },
      {
        "op": "connect",
        "from": "Webhook",
        "to": "Forward to API"
      }
    ]
  },
  "validation": {
    "expectedNodes": 2,
    "expectedConnections": 1,
    "mustHaveNodes": ["Webhook", "Forward to API"]
  }
}