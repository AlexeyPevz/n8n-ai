#############################
# n8n-ai Monorepo .env.example
#############################

# --- Common ports ---
N8N_PORT=5678
ORCH_PORT=3000
REDIS_PORT=6379
QDRANT_PORT=6333

# --- n8n-ai-orchestrator ---
PORT=3000
LOG_LEVEL=info
N8N_URL=http://localhost:5678
N8N_WEBHOOK_BASE=http://localhost:5678
USE_HOOKS=1
USE_HOOKS_VALIDATE=1
USE_HOOKS_SIMULATE=1
HOOKS_FETCH_RETRIES=2
HOOKS_FETCH_TIMEOUT_MS=3000

# Policies
DIFF_POLICY_MAX_ADD_NODES=10
DIFF_POLICY_DOMAIN_BLACKLIST=

# Security & CORS
SECURITY_PRESET=development
SECURITY_API_KEYS=
SECURITY_RATE_LIMIT=100
SECURITY_RATE_WINDOW=60000
SECURITY_TRUSTED_PROXIES=
APP_URL=http://localhost:3001
CORS_ORIGINS=http://localhost:3001,http://localhost:5678
JWT_SECRET=dev-secret
MAX_BODY_SIZE=5242880

# Git integration (optional)
GIT_INTEGRATION_ENABLED=false
GIT_REPO_PATH=
GIT_BRANCH=main
GIT_REMOTE=origin
GIT_AUTHOR_NAME=AI Workflow Builder
GIT_AUTHOR_EMAIL=ai@n8n.local
GIT_PR_ENABLED=true
GIT_PR_BASE=main
GIT_PROVIDER=github
GIT_PR_ASSIGNEES=
GIT_PR_REVIEWERS=
GIT_PR_LABELS=

# Schema cache
SCHEMA_CACHE_SIZE=1000
SCHEMA_CACHE_TTL=3600000

# --- n8n-ai-hooks (n8n) ---
N8N_AI_ENABLED=true
N8N_AI_ORCHESTRATOR_URL=http://localhost:3000
N8N_AI_API_TOKEN=
N8N_AI_RATELIMIT_WINDOW_MS=15000
N8N_AI_RATELIMIT_MAX=60

# n8n core (dev defaults)
N8N_ENCRYPTION_KEY=dev-key
N8N_HOST=localhost
N8N_PROTOCOL=http
N8N_WEBHOOK_URL=http://localhost:5678/

# Panel/UI
UI_BASE=http://localhost:3001

# n8n Configuration
N8N_PORT=5678
N8N_ENCRYPTION_KEY=your-encryption-key
N8N_HOST=localhost
N8N_WEBHOOK_URL=http://localhost:5678/

# Orchestrator Configuration
ORCH_PORT=3000
REDIS_URL=redis://redis:6379
QDRANT_URL=http://qdrant:6333
N8N_URL=http://n8n:5678

# AI Configuration
# Provider: openai, anthropic, ollama, openrouter
AI_PROVIDER=openai
AI_API_KEY=your-openai-api-key
AI_MODEL=gpt-4-turbo-preview
AI_TEMPERATURE=0.3
AI_MAX_TOKENS=4000

# OpenRouter specific (optional)
# OPENROUTER_SITE_URL=https://your-app.com
# OPENROUTER_SITE_NAME=n8n-ai

# Optional: Fallback AI provider
# AI_FALLBACK_PROVIDER=anthropic
# AI_FALLBACK_API_KEY=your-anthropic-api-key
# AI_FALLBACK_MODEL=claude-3-sonnet-20240229

# Example: Using OpenRouter as primary
# AI_PROVIDER=openrouter
# AI_API_KEY=your-openrouter-api-key
# AI_MODEL=anthropic/claude-3-opus
# AI_MODEL=google/gemini-pro
# AI_MODEL=meta-llama/llama-3-70b-instruct
# AI_MODEL=mistralai/mixtral-8x7b-instruct

# AI Features
AI_RAG_ENABLED=true
AI_RAG_COLLECTION=n8n-docs
AI_RAG_TOP_K=5
AI_CACHE_ENABLED=true
AI_CACHE_TTL=3600
AI_STREAMING=false

# Security
N8N_AI_API_TOKEN=your-api-token
N8N_AI_RATELIMIT_WINDOW_MS=15000
N8N_AI_RATELIMIT_MAX=60

# Development
LOG_LEVEL=debug
NODE_ENV=development