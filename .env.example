# n8n Configuration
N8N_PORT=5678
N8N_HOST=localhost
N8N_PROTOCOL=http
N8N_ENCRYPTION_KEY=your-encryption-key-here
NODE_FUNCTION_ALLOW_EXTERNAL=*

# AI Features
N8N_AI_ENABLED=true
N8N_AI_ORCHESTRATOR_MODE=embedded
N8N_AI_UI_POSITION=bottom

# AI Providers (at least one required)
# OpenAI
OPENAI_API_KEY=sk-...

# Anthropic (optional)
# ANTHROPIC_API_KEY=sk-ant-...

# OpenRouter (optional)
# OPENROUTER_API_KEY=sk-or-...

# Security
N8N_AI_API_TOKEN=your-secure-api-token
JWT_SECRET=your-jwt-secret
SECURITY_API_KEYS=key1,key2

# Rate Limiting
N8N_AI_RATELIMIT_WINDOW_MS=15000
N8N_AI_RATELIMIT_MAX=60
AI_RATE_LIMIT=10

# CORS
CORS_ORIGINS=http://localhost:5678,http://localhost:3000

# Features
WORKFLOW_MAP_ENABLED=true
AUDIT_LOG_ENABLED=true
GIT_INTEGRATION_ENABLED=false

# Database (optional)
# DB_TYPE=postgresdb
# DB_POSTGRESDB_HOST=localhost
# DB_POSTGRESDB_PORT=5432
# DB_POSTGRESDB_DATABASE=n8n
# DB_POSTGRESDB_USER=n8n
# DB_POSTGRESDB_PASSWORD=n8n

# Redis (for caching)
REDIS_URL=redis://redis:6379

# Qdrant (for RAG)
QDRANT_URL=http://qdrant:6333
# QDRANT_API_KEY=

# Orchestrator Settings
ORCHESTRATOR_PORT=3000
USE_HOOKS=1
HOOKS_FETCH_RETRIES=2
HOOKS_FETCH_TIMEOUT_MS=3000

# Schema Cache
SCHEMA_CACHE_SIZE=1000
SCHEMA_CACHE_TTL=3600000

# Diff Policies
DIFF_POLICY_MAX_ADD_NODES=10
DIFF_POLICY_DOMAIN_BLACKLIST=

# Critic Settings
CRITIC_MAX_TRIES=3

# Logging
LOG_LEVEL=info
NODE_ENV=production