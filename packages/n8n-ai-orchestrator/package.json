{
  "name": "@n8n-ai/orchestrator",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "lint": "eslint --ext .ts src",
    "test": "vitest",
    "test:e2e": "vitest run src/e2e.test.ts",
    "test:with-server": "tsx src/test-server.ts & SERVER_PID=$! && sleep 2 && vitest run src/e2e.test.ts; kill $SERVER_PID"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@n8n-ai/schemas": "workspace:*",
    "fastify": "^4.28.1"
  },
  "devDependencies": {
    "@types/node": "20",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "vitest": "^3.2.4"
  }
}
