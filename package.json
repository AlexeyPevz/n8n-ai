{
  "name": "n8n-ai",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "pnpm -r run build",
    "dev": "pnpm -r --stream --parallel run dev",
    "dev:full": "docker compose up -d && pnpm -r --stream --parallel run dev",
    "lint": "pnpm -r run lint",
    "lint:fix": "pnpm -r run lint -- --fix",
    "test": "pnpm -r run test",
    "test:ci": "pnpm run lint && pnpm run test && pnpm run test:golden",
    "test:golden": "cd examples && pnpm test",
    "e2e": "pnpm -C packages/n8n-ai-orchestrator test:e2e",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "check:types": "pnpm -r exec tsc --noEmit",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.3.0",
    "@commitlint/config-conventional": "^18.3.0",
    "@n8n/client-oauth2": "^0.7.0",
    "@types/node": "20",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "n8n-core": "^1.14.1",
    "n8n-design-system": "^0.60.0",
    "n8n-workflow": "^1.71.0",
    "prettier": "^3.3.3",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4"
  }
}
